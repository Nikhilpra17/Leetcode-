  public int maximumWealth(int[][] accounts) {
        
        int richest =0 ;
        
        for (int i =0; i< accounts.length ; i++){
            int sum =0;
            for (int k =0 ; k < accounts[0].length; k ++){
                sum += accounts[i][k];
            }
            if (sum > richest){
                richest = sum;
            }
        }
        return richest;
    }
}
