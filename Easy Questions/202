class Solution {
    public boolean isHappy(int n) {
        int slow = n ;
        int fast = n;
        
        do {
            slow = findSq(slow);
            fast = findSq(findSq(fast));
        }while (slow != fast);
        
        if (slow == 1){
            return true;
        } 
        return false;
    }
    
