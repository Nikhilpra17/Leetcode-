class Solution {
    public boolean canPartition(int[] nums) {
        int sum = 0 ; 
        
        for (int i = 0 ; i < nums.length ; i++){
            sum += nums[i] ; 
        }
        
        if (sum % 2 != 0){
            return false;
        }
        
        int range = sum / 2 ;
        
        
        return subSet(nums, range, nums.length);
        
    }
