class Solution {
    public int minDistance(String word1, String word2) {
        int len1 = word1.length();
        int len2 = word2.length();
        
        int LCSint = LCS(word1, len1, word2, len2);
        
        int ans = (len1 - LCSint) + (len2 - LCSint);
        
        return ans;
        
    }
